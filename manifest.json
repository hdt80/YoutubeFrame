{
    "manifest_version": 2,

    "name": "YoutubeFrame",
    "description": "Single frame movement",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "YoutubeFrame"
    },

    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/watch*"],
            "js": ["content_script.js"]
        }
    ],

    "web_accessible_resources": [
        "youtube_frame.js"
    ]
}